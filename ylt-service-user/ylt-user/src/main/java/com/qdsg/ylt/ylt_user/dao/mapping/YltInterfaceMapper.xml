<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qdsg.ylt.common.persistence.dao.YltInterfaceMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.qdsg.ylt.ylt_user.common.model.YltInterface">
        <id column="id" property="id" />
        <result column="hospital_id" property="hospitalId" />
        <result column="hos_app_id" property="hosAppId" />
        <result column="hos_app_sec" property="hosAppSec" />
        <result column="app_id" property="appId" />
        <result column="app_sec" property="appSec" />
        <result column="get_token_url" property="getTokenUrl" />
        <result column="get_token_service" property="getTokenService" />
        <result column="op_user_id" property="opUserId" />
        <result column="status" property="status" />
        <result column="create_date" property="createDate" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, hospital_id AS hospitalId, hos_app_id AS hosAppId, hos_app_sec AS hosAppSec, app_id AS appId, app_sec AS appSec, get_token_url AS getTokenUrl, get_token_service AS getTokenService, op_user_id AS opUserId, status, create_date AS createDate
    </sql>

</mapper>
